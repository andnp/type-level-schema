{
  "name": "type-level-schema",
  "version": "0.0.0",
  "description": "Typescript typings for JSON schemas",
  "main": "src/index.d.ts",
  "scripts": {
    "lint": "tslint --config tslint.json --project . --format stylish",
    "test": "jest",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andnp/type-level-schema.git"
  },
  "keywords": [
    "typescript",
    "types",
    "JSON-schema",
    "schema"
  ],
  "author": "Andy Patterson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andnp/type-level-schema/issues"
  },
  "homepage": "https://github.com/andnp/type-level-schema#readme",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/tests/**/*.test.ts?(x)"
    ],
    "moduleDirectories": [
      "node_modules",
      "./src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/ajv": "^1.0.0",
    "@types/jest": "^22.2.2",
    "@types/jsonschema": "^1.1.1",
    "ajv": "^6.4.0",
    "jest": "^22.4.3",
    "jsonschema": "^1.2.4",
    "pre-push": "^0.1.1",
    "ts-jest": "^22.4.2",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1"
  },
  "pre-push": [
    "lint",
    "tsc",
    "test"
  ],
  "dependencies": {
    "simplytyped": "^1.0.3"
  }
}
